<!DOCTYPE html>
<html>
        <head>
				<script src="/socket.io/socket.io.js"></script>
				<link rel="stylesheet" type="text/css" href="/bower_components/bootstrap/dist/css/bootstrap.min.css"> 
                <title>login</title>
                <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
                <script type="text/javascript">
                	$(window).ready(function() {
                		var socket = io();
               			socket.emit('checklogin', {'user': `<%= name %>`});
 
                		socket.on('checklogin', function(data){
                			alert(`<%= name %> da login`);
                			window.location = '/logout';
                		});
                	});
                </script>
        </head>
        <body>
		<h1>Hello : <%= name %></h1>
		<% if (administrator == 1) { %>
			<a class="navbar-brand list" href="/admin">< Login Admin</a>
		<% } %>
		<table class="table table-hover">
		  <thead>
		  	<tr>
		  		<th>Name</th>
		  		<!-- <th>Url</th> -->
		  		<th>Status</th>
		  	</tr>
		  </thead>
		  <tbody class="tbody1">
		  <% content.forEach(function(data){ %>
		<tr>
			<th><a href="/project/<%- JSON.stringify(data.name).replace(/"/g, '') %>"><%- JSON.stringify(data.name).replace(/"/g, '') %></a></th>
<!-- 			<th><%- JSON.stringify(data.url).replace(/"/g, '') %></th> -->
			<th><%- JSON.stringify(data.color).replace(/"/g, '') %></th>
		</tr>
		<% }) %>
		  </tbody>
		</table>
	</body>
</html>


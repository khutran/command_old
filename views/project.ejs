<!DOCTYPE html>
<html lang="">
	<link rel="stylesheet" type="text/css" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="/assets/css/app.css">
	<script src="/socket.io/socket.io.js"></script>
	<script src="https://code.jquery.com/jquery-1.11.1.js"></script>
	<script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
	<script src="/assets/js/app.js"></script>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>Project</title>

		<!-- Bootstrap CSS -->
		<link rel="stylesheet" href="/assets/css/style.css" crossorigin="anonymous">
		<link rel="stylesheet" href="/assets/css/font-awesome/css/font-awesome.min.css" crossorigin="anonymous">
	</head>
	<body>
		<div class="header">
			<nav class="navbar navbar-default" role="navigation">
				<div class="container">
					<!-- Brand and toggle get grouped for better mobile display -->
					<div class="navbar-header">
						<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
						<a class="navbar-brand list" href="/user">< Project List</a>
					</div>
			
					<!-- Collect the nav links, forms, and other content for toggling -->
					<div class="collapse navbar-collapse navbar-ex1-collapse">

						<ul class="nav navbar-nav navbar-right">
							<li><a href="#" id="user">Hello: <span><%= user %></a></li>
							<li class="dropdown">
								<a href="/logout" class="logout"><i class="fa fa-sign-in" aria-hidden="true"></i> Logout</a>
							</li>
						</ul>
					</div><!-- /.navbar-collapse -->
				</div>
			</nav>
		</div>
	
	<!-- end header -->
		<div class="main">
			<div class="container">
				<h4>Project: <span id="domain"><%= domain %><img class="color" src="/assets/images/<%= color %>.png"></span></h4>
				<div class="row">				
					<div class="col-md-9">						
						<div class="themes">
                            <ul class="loads" id="loads0"></ul>
							<ul class="loads" id="loads1"></ul>
							<ul class="loads" id="loads2"></ul>
						</div>
						<div class="text-last">
							<button type="submit" id="back">Back</button>
							<button type="submit" id="wordpress">cd themes</button>
						</div>
						<div class="command">
							<div class="text">
								<p>Command:</p>
							</div>
							<div class="textarea">
								<input type="text" name="" id="command">
							</div>
							<div class="button">
								<button type="button" id="send">Run command</button>
							</div>						
						</div>
						<div class="button ">
							<button type="button" class="buid-project" id="logs">Show log</button>
						</div>
<!-- 						<div class="button ">
							<button type="button" class="buid-project" id="out_rooms">out rooms</button>
						</div> -->
						<div class="button">
							<button type="button" class="buid-project" id="atactbuild" data-toggle="modal" data-target="#myModal1">Buid Project</button>
						</div>
						<div class="button">
							<button type="button" class="dowload download">Dowload Database</button>
						</div>
						<div class="button">
							<button type="button" class="bitbucket">bitbucket</button>
						</div>
						<div class="row">
							<div class="col-md-12 ">
                                    <img id="img" src="/assets/images/l.gif">
									<ul id='results'>Results...</ul>
							</div>
						</div>
					</div>

					<div class="col-md-3">
						<div class="online-user">
							<h2>
								<i class="fa fa-users" aria-hidden="true"></i> Online Users</h2>
							<ul class="list-group showuser">
							</ul>
						</div>
						
					</div>
				</div>
			</div>
		</div>
            <!-- Button trigger modal -->
            <!-- Modal -->
            <div class="modal fade" id="myModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Build Project</h4>
                        </div>
                        <div class="modal-body">
                            <input class="composer" type="checkbox">run composer</input><input class="database" type="checkbox">import database</input>
                            <img id="img1" src="/assets/images/l.gif">
                            <p><span class="status"></span><span class="number"></span><button type="button" class="btn btn-primary log">view log</button></p>
                            <!-- <span>import database</span> -->
                            <p class="logapi"></p>
                            <p class="viewlog"></p>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-primary build">Build</button>
                        </div>
                    </div>
                </div>

	        </div>
	        
		<script src="/assets/js/jquery.min.js"></script>
		<script src="/assets/js/bootstrap.min.js"></script>
	</body>
</html>